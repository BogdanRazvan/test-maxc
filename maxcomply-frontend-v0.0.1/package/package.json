{
  "name": "@maxcomply/frontend",
  "private": true,
  "version": "0.0.1",
  "description": "Maxcomply shared frontend",
  "repository": "https://bitbucket.org:maxcomply/maxc-frontend.git",
  "author": "Max Comply",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/frontend.umd.cjs",
  "module": "./dist/frontend.js",
  "exports": {
    ".": {
      "import": "./dist/frontend.js",
      "require": "./dist/frontend.umd.cjs"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:dev": "yarn build --mode development",
    "build:dev:sourcemap": "yarn build:dev --sourcemap true",
    "build:sourcemap": "yarn build --sourcemap true",
    "preview": "vite preview",
    "optimize": "vite optimize",
    "optimise": "yarn optimize",
    "analyze": "yarn build --config analyze.config.js",
    "bundle": "yarn install && yarn build && yarn pack",
    "bundle:simple": "yarn build && yarn pack",
    "bundle:dev": "yarn build:dev:sourcemap && yarn pack",
    "lint:test": "eslint --ext .js,.vue --ignore-path .gitignore src",
    "lint": "eslint --ext .js,.vue,.cjs --ignore-path .gitignore --fix src",
    "lint:styles": "stylelint -f verbose '**/*.{css,scss,sass}' --fix",
    "lint:all": "yarn lint:styles && yarn lint",
    "test:unit": "vitest run",
    "test:unit:watch": "vitest watch",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui",
    "storybook": "storybook dev -p 40969",
    "storybook:sslfallback": ". /usr/local/bin/command.conf && NODE_OPTIONS=--openssl-legacy-provider storybook dev -h $IP -p 40969 --no-open",
    "storybook:sslfallback:host": ". /usr/local/bin/command.conf && NODE_OPTIONS=--openssl-legacy-provider storybook dev -h sandbox.$DOMAIN -p 40969 --no-open",
    "build-storybook": "storybook build",
    "build-storybook:sslfallback": "NODE_OPTIONS=--openssl-legacy-provider storybook build",
    "postinstall": "husky install"
  },
  "peerDependencies": {
    "vue": "^3.3.4"
  },
  "dependencies": {
    "@highlightjs/vue-plugin": "^2.1.0",
    "@quasar/quasar-ui-qmediaplayer": "^2.0.0-beta.6",
    "@storybook/addon-designs": "^7.0.0",
    "@tinymce/tinymce-vue": "^5.0.0",
    "apexcharts": "^3.40.0",
    "axios": "^1.4.0",
    "colortranslator": "^2.0.0",
    "countup.js": "^2.3.2",
    "highlight.js": "^11.8.0",
    "html-to-pdfmake": "^2.4.20",
    "html2canvas": "^1.4.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "pdfmake": "^0.2.7",
    "quasar": "^2.12.0",
    "sass": "^1.55.0",
    "storybook-addon-designs": "^6.3.1",
    "tinymce": "^5.10.2",
    "vue": "^3.3.4",
    "vue-countup-v2": "^4.0.0",
    "vue-i18n": "^9.2.2",
    "vue3-apexcharts": "^1.4.1",
    "vue3-signature-pad": "^1.0.7",
    "vuedraggable": "^4.1.0"
  },
  "devDependencies": {
    "@quasar/quasar-app-extension-testing-unit-vitest": "^0.2.1",
    "@quasar/vite-plugin": "^1.3.3",
    "@storybook/addon-a11y": "^7.0.26",
    "@storybook/addon-actions": "^7.1.0-beta.2",
    "@storybook/addon-essentials": "^7.1.0-beta.2",
    "@storybook/addon-interactions": "^7.1.0-beta.2",
    "@storybook/addon-links": "^7.1.0-beta.2",
    "@storybook/addon-mdx-gfm": "^7.1.0-beta.2",
    "@storybook/testing-library": "^0.2.0",
    "@storybook/vue3": "^7.1.0-beta.2",
    "@storybook/vue3-vite": "^7.1.0-beta.2",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-istanbul": "^0.31.1",
    "@vitest/ui": "^0.31.1",
    "@vue/compiler-sfc": "^3.2.45",
    "@vue/test-utils": "^2.3.2",
    "eslint": "^8.41.0",
    "eslint-plugin-vue": "^9.14.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "jsdom-testing-mocks": "^1.9.0",
    "pinia": "^2.1.3",
    "postcss": "^8.4.23",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rollup-plugin-visualizer": "^5.9.0",
    "storybook": "^7.1.0-beta.2",
    "stylelint": "^15.6.2",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^9.0.0",
    "stylelint-scss": "^5.0.0",
    "stylelint-selector-bem-pattern": "^2.1.1",
    "ts-loader": "^9.4.2",
    "typescript": "^5.0.4",
    "vite": "^4.3.7",
    "vitest": "^0.31.1"
  }
}
